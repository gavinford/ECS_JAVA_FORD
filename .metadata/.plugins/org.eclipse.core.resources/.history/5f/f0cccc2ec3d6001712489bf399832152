package variables;

public class yourschedule 
{
	public static void main(String[] args)
	{
		String subject1,teacher1,subject2,teacher2,subject3,teacher3,subject4,teacher4;
		
		subject1 = "Spanish 1";
		teacher1 = "Gomez";
		subject2 = "ECS";
		teacher2 = "Mauro";
		subject3 = "Biology";
		teacher3 = "Lawless";
		subject4 = "PE";
		teacher4 = "Lackey";
		
		System.out.println("Ford's Class Schedule:");
		
		System.out.println();
		System.out.println();
		
		System.out.println("----------------------------------------------------------");
		System.out.println("|Per 1 |      "+subject1+"      |      "+teacher1+"      |");
		System.out.println("|Per 2 |      "+subject2+"            |      "+teacher2+"      |");
		System.out.println("|Per 3 |      "+subject3+"        |      "+teacher3+"      |");
		System.out.println("|Per 4 |      "+subject4+"             |      "+teacher4+"      |");
		System.out.println("----------------------------------------------------------");
	}

}
