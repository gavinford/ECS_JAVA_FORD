package variables;
public class usingvariables 
{
	public static void main(String[] args)
	{
		int room;
		double e;
		
		room = 113;
		e = 2.71828;

		System.out.println("This is room # " + room);
		System.out.println("e is close to " + e);
		System.out.println("I am learning a bit about ");
		
		
	}
}