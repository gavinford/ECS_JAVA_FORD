package yourintials;

public class yourintials 
{
	public static void main ( String[] args)
	{
		System.out.println("Gavin Angus Ford");
		System.out.println(" GGG     A    FFFFF");
		System.out.println("G   G   A A   F    ");
		System.out.println("G      A   A  F    ");
		System.out.println("G  GG  AAAAA  FFF  ");
		System.out.println("G   G  A   A  F    ");
		System.out.println("G   G  A   A  F    ");
		System.out.println(" GGG   A   A  F    ");
	}
}
